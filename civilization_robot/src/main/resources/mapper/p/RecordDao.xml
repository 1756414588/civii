<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.game.dao.p.RecordDao">

  <select id="load" resultType="com.game.domain.Record">
        select * from p_record
  </select>

  <insert id="insert" parameterType="com.game.domain.Record">
        insert into p_record(accountKey,recordId,`state`,createTime) values(#{accountKey},#{recordId},#{state},#{createTime})
  </insert>

  <update id="update" parameterType="com.game.domain.Record">
        update p_record set recordId = #{recordId}, `state` = #{state}, createTime=#{createTime} where accountKey = #{accountKey}
  </update>


</mapper>